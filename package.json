{
	"name": "main-crawler-project",
	"version": "1.0.0",
	"scripts": {
		"dev": "concurrently \"npm run frontend:dev\" \"npm run backend:dev\"",
		"frontend:dev": "cd frontend && npm run dev",
		"frontend:build": "cd frontend && npm run build",
		"frontend:preview": "cd frontend && npm run preview",
		"backend:dev": ".venv\\Scripts\\python.exe start_backend.py",
		"backend:start": ".venv\\Scripts\\python.exe start_backend.py",
		"build": "npm run frontend:build",
		"start": "npm run backend:start",
		"install:frontend": "cd frontend && npm install",
		"install:all": "npm install && npm run install:frontend"
	},
	"dependencies": {
		"@apollo/server": "^5.0.0",
		"@graphql-codegen/cli": "^5.0.7",
		"@graphql-codegen/typescript": "^4.1.6",
		"@graphql-codegen/typescript-resolvers": "^4.5.1",
		"@graphql-tools/merge": "^9.1.1",
		"@graphql-tools/schema": "^10.0.25",
		"@graphql-tools/utils": "^10.9.1",
		"@radix-ui/react-select": "^2.2.6",
		"@radix-ui/react-slot": "^1.2.3",
		"@radix-ui/react-toast": "^1.2.15",
		"@supabase/supabase-js": "^2.55.0",
		"apollo-server-express": "^3.13.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"cors": "^2.8.5",
		"dataloader": "^2.2.3",
		"express": "^4.17.1",
		"graphql": "^16.11.0",
		"graphql-middleware": "^6.1.35",
		"graphql-shield": "^7.6.5",
		"graphql-validation-complexity": "^0.4.2",
		"helmet": "^8.1.0",
		"lucide-react": "^0.540.0",
		"tailwind-merge": "^3.3.1",
		"tailwindcss-animate": "^1.0.7"
	},
	"devDependencies": {
		"concurrently": "^9.2.0"
	}
}
